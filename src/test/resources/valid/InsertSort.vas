class Main is
    this() is
        var n : Integer = 10
        var a : Array[Integer] = Array[Integer](n)
        a.set(0, 4)
        a.set(1, 1)
        a.set(2, 5)
        a.set(3, 0)
        a.set(4, 3)
        a.set(5, 8)
        a.set(6, 7)
        a.set(7, 9)
        a.set(8, 6)
        a.set(9, 2)

        // https://rosettacode.org/wiki/Sorting_algorithms/Insertion_sort#Java
        var i : Integer = 1
        while i.less(n) loop
            var value : Integer = a.get(i)
            var j : Integer = i.minus(1)
            while j.greaterEqual(0).and(a.get(j).greater(value)) loop
                a.set(j.plus(1), a.get(j))
                j = j.minus(1)
            end
            a.set(j.plus(1), value)
            i = i.plus(1)
        end

        var i : Integer = 0
        while i.lessEqual(n) loop
            if a.get(i).equal(i).not() then
                // TODO: error
            end
            i = i.plus(1)
        end
    end
end
