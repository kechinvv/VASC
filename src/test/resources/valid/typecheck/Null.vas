class Parent is
    this(i: Integer) is
    end
end

class Derived extends Parent is

    var i : Integer = null

    this(i: Integer) is
        i = null
    end

    method returnNull() : Integer is
        return null
    end

    method acceptNull(a : Real,  b : Boolean) is
        this(null)
        acceptNull(null, null)
    end

end